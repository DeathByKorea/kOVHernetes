{
    "apiVersion": "v1",
    "kind": "Config",
    "current-context": "ovh",
    "clusters": [
        {
            "name": "ovh",
            "cluster": {
                "server": "https://localhost:6443",
                "certificate-authority": "tls/ca.pem"
            }
        }
    ],
    "users": [
        {
            "name": "ovh",
            "user": {
                "client-certificate": "tls/k8s_c.crt",
                "client-key": "tls/k8s_c.key"
            }
        }
    ],
    "contexts": [
        {
            "name": "ovh",
            "context": {
                "cluster": "ovh",
                "user": "ovh"
            }
        }
    ]
}
